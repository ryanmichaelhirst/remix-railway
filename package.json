{
  "name": "remix-railway-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:routes build:remix build:server",
    "build:routes": "routes-gen -d @routes-gen/remix",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "clean": "rm -rf .cache build public/build && find ./ -name \".DS_Store\" -print -delete",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --watch-path ./build/server.js --watch ./build/server.js\"",
    "dev:routes": "routes-gen -d @routes-gen/remix",
    "dev:postgres": "docker-compose -f docker-compose.dev.yaml up -d",
    "deploy:db": "prisma migrate deploy",
    "docker": "docker-compose -f docker-compose.dev.yaml up -d",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "tunnel": "cloudflared tunnel --config cloudflare.yaml run",
    "typecheck": "tsc"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@remix-run/css-bundle": "*",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@routes-gen/remix": "^0.3.7",
    "@stripe/react-stripe-js": "^2.7.0",
    "@stripe/stripe-js": "^3.3.0",
    "chokidar": "^3.5.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "framer-motion": "^11.1.7",
    "isbot": "^3.7.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.363.0",
    "micro": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-google": "^2.0.0",
    "remix-auth-oauth2": "^1.11.2",
    "remix-flat-routes": "^0.6.4",
    "remix-typedjson": "^0.4.1",
    "remix-utils": "^7.6.0",
    "routes-gen": "^0.6.1",
    "source-map-support": "^0.5.21",
    "stripe": "^15.4.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.1",
    "ts-pattern": "^5.0.8",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.22.4",
    "zodix": "^0.4.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@remix-run/dev": "^2.8.1",
    "@types/eslint": "^8.44.6",
    "@types/express": "^4.17.20",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.18.6",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/source-map-support": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.14",
    "prisma": "^5.7.1",
    "tailwindcss": "3.4.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx -r tsconfig-paths/register prisma/seed.ts"
  }
}
